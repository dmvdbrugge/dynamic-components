{
    "name": "dmvdbrugge/dynamic-components",
    "description": "Dynamic Components for the PHP UI extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave van der Brugge",
            "email": "dmvdbrugge@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "ext-ui": "*"
    },
    "autoload": {
        "psr-4": {
            "DynamicComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^7"
    }
}
